<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mainbox.fiscaluno">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.main.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.cadastro.CadastroNomeActivity" />
        <activity android:name=".view.cadastro.CadastroInstituicaoActivity" />
        <activity android:name=".view.cadastro.CadastroPontosPositivosActivity" />
        <activity android:name=".view.cadastro.CadastroPontosNegativosActivity" />
        <activity android:name=".view.cadastro.AvaliacoesActivity" />
        <activity android:name=".view.cadastro.ConselhosActivity" />
        <activity android:name=".view.cadastro.AvaliacaoGeralActivity"/>
    </application>

</manifest>