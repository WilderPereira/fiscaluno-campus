<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/rl_frame_pesquisa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <include layout="@layout/frame_pesquisa" />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl_frame_lista"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl_frame_pesquisa">

        <include
            android:id="@+id/frm_lista"
            layout="@layout/frame_pesquisa_recente" />

    </RelativeLayout>

</RelativeLayout>